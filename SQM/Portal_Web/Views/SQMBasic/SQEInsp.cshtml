@Scripts.Render("~/bundles/SQMBasic/SQEInsp")

@RenderPage("SQEInsp/InspCode/dialogCode.cshtml")
@RenderPage("SQEInsp/InspDesc/InspDesc.cshtml")
@RenderPage("SQEInsp/InspVar/InspVar.cshtml")
<div id="inspCode">
    <div id="tbMain1Code" class="ui-widget-header ui-corner-all" style="width:570px; padding:3px 15px 3px 15px; font-size:10.5px; display:none;">
        <button id="btnShowDesc"></button>
        <button id="btnCreateCode"></button>
        <button id="btnViewEditCode"></button>
        <button id="btnDeleteCode"></button>
    </div>

    <div>
        <table>
            <tr>
                <td>檢測項目:</td>
                <td>
                    <input type="text" id="txtFilterTextCode" value="" />
                </td>
                <td class="FieldLabel">檢驗結果類型：</td>
                <td>
                    <select id="ddlSInsptype" style="width: 204px; display: inline-block;">
                        <option value="">-請選擇-</option>
                        <option value="Attributes">檢驗結果-計數值 (Inspection Result - Attributes)</option>
                        <option value="Variables">檢驗結果-計量植 (Inspection Result - Variables)</option>
                    </select>
                </td>
                <td>
                <button id="btnSearchCode"></button>
                </td>
            </tr>
        </table>
    </div>

    <div>
        <table id="gridDataListCode"></table>
        <div id="gridListPagerCode"></div>
    </div>
</div>