<div id="dialogPlantForwarderInfo" title="Plant Forwarder Info" class="JQDialog" style="display:none">
    <div>
        <table style="width:100%;" border="0">
            <tr>
                <td class="tdTitleAlignRight" style="width:20%;" nowrap>Plant:</td>
                <td class="tdTitleAlignLeft"><input type="text" id="txtPlant" value="" maxlength="10" style="width:100%"></td>
            </tr>
            <tr>
                <td class="tdTitleAlignRight" nowrap>Forwarder Company Name:</td>
                <td class="tdTitleAlignLeft"><input type="text" id="txtCompanyName" value="" maxlength="200" style="width:100%"></td>
            </tr>
            <tr>
                <td class="tdTitleAlignRight" nowrap>Telephone:</td>
                <td class="tdTitleAlignLeft"><input type="text" id="txtTel" value="" maxlength="50" style="width:100%"></td>
            </tr>
            <tr>
                <td class="tdTitleAlignRight" nowrap>Contact Person:</td>
                <td class="tdTitleAlignLeft"><input type="text" id="txtName" value="" maxlength="50" style="width:100%"></td>
            </tr>
            <tr>
                <td class="tdTitleAlignRight" nowrap>Email:</td>
                <td class="tdTitleAlignLeft"><input type="text" id="txtEmail" value="" maxlength="200" style="width:100%"></td>
            </tr>
            <tr>
                <td class="tdTitleAlignRight" nowrap>Address:</td>
                <td class="tdTitleAlignLeft"><input type="text" id="txtAddress" value="" maxlength="200" style="width:100%"></td>
            </tr>
        </table>
    </div>
</div>
