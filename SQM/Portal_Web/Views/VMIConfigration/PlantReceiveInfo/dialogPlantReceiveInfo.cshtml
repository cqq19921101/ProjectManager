<div id="dialogPlantReceiveInfo" title="Plant Receive Info" class="JQDialog" style="display:none">
    <div>
        <table style="width:100%;" border="0">
            <tr>
                <td class="tdTitleAlignRight" style="width:20%;" nowrap>Plant:</td>
                <td class="tdTitleAlignLeft"><input type="text" id="txtPlant" value="" maxlength="4" style="width:100%"></td>
            </tr>
            <tr>
                <td class="tdTitleAlignRight" nowrap>Receiver:</td>
                <td class="tdTitleAlignLeft"><input type="text" id="txtReceiver" value="" maxlength="50" style="width:100%"></td>
            </tr>
            <tr>
                <td class="tdTitleAlignRight" nowrap>Receive Address:</td>
                <td class="tdTitleAlignLeft"><input type="text" id="txtReceiveAddr" value="" maxlength="200" style="width:100%"></td>
            </tr>
            <tr>
                <td class="tdTitleAlignRight" nowrap>Receiver Telephone:</td>
                <td class="tdTitleAlignLeft"><input type="text" id="txtReceiverTel" value="" maxlength="60" style="width:100%"></td>
            </tr>
        </table>
    </div>
</div>
