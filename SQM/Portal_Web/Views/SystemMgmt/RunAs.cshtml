@*@{
    ViewBag.Title = "RunAs";
}*@

@Scripts.Render("~/bundles/SystemMgmt/RunAs")

<div id="tbMain1" class="ui-widget-header ui-corner-all" style="width:570px; padding:3px 15px 3px 15px; font-size:10.5px; display:none;">
    <button id="btnSearch"></button>
    <button id="btnRunAs"></button>
</div>

<div>
    <table>
        <tr>
            <td>Search Text:</td>
            <td>
                <input type="text" id="txtFilterText" value="" />
            </td>
        </tr>
        <tr>
            <td>Member Type:</td>
            <td>
                <span><input type="radio" name="FilterMemberType" id="radFilterMemberTypeAll" checked />All</span>
                <span><input type="radio" name="FilterMemberType" id="radFilterMemberTypeInternalOnly" />Internal Only</span>
                <span><input type="radio" name="FilterMemberType" id="radFilterMemberTypeExternalOnly" />External Only</span>
            </td>
        </tr>
    </table>
</div>

<div>
    <table id="gridDataList"></table>
    <div id="gridListPager"></div>
</div>
<span id="txtRunAsResult" class="ErrorMsg">@ViewBag.Message</span>
<form id="_RunAsForm" method="post"><input type="hidden" id="hidRunAsMemberGUID" name="RunAsMemberGUID" /></form>